# API Keys
DASHSCOPE_API_KEY=your-dashscope-api-key-here

# Server
BACKEND_PORT=8010
FRONTEND_PORT=3060
HOST=0.0.0.0

# Model Caching (load models at server startup for high concurrency support)
ENABLE_MODEL_CACHE=true
USE_CUDA_FOR_RETRIEVAL=true

# Generation Settings
DEFAULT_MODEL=qwen3-vl-flash
RETRIEVAL_TOPK=50
RETRIEVAL_TOPM=10
RETRIEVAL_ALPHA=0.8
TIMEOUT=300

# Security
MAX_FILE_SIZE_MB=100
MAX_REQUESTS_PER_MINUTE=10
