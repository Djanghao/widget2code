# API Keys
DASHSCOPE_API_KEY=your-dashscope-api-key-here

# Server
BACKEND_PORT=8010
FRONTEND_PORT=3060
HOST=0.0.0.0

# Model Caching (load models at server startup for high concurrency support)
ENABLE_MODEL_CACHE=true
USE_CUDA_FOR_RETRIEVAL=true

# Generation Settings
DEFAULT_MODEL=qwen3-vl-flash
RETRIEVAL_TOPK=50
RETRIEVAL_TOPM=10
RETRIEVAL_ALPHA=0.8
TIMEOUT=300

# Icon Libraries Configuration
# Default: all 57 available libraries
# Minimal (popular):    ICON_LIB_NAMES='["lucide", "heroicons-outline"]'
# Apple ecosystem:      ICON_LIB_NAMES='["sf"]'
# All libraries grouped by family:
ICON_LIB_NAMES='["lucide", "sf", "core-bold", "core-duotone", "core-fill", "core-light", "core-regular", "core-thin", "heroicons-outline", "heroicons-solid", "ant-design-filled", "ant-design-outlined", "ant-design-twotone", "boxicons-logos", "boxicons-regular", "boxicons-solid", "font-awesome-brands", "font-awesome-regular", "font-awesome-solid", "remix-arrows", "remix-buildings", "remix-business", "remix-communication", "remix-design", "remix-development", "remix-device", "remix-document", "remix-editor", "remix-finance", "remix-food", "remix-health-medical", "remix-logos", "remix-map", "remix-media", "remix-others", "remix-system", "remix-user-faces", "remix-weather", "tabler-filled", "tabler-outline", "circum", "codicons", "css-gg", "devicons", "feather", "flat-color", "grommet", "icomoon", "icons-lib", "ionicons", "octicons", "radix", "simple-icons", "simple-line-icons", "themify", "typicons", "weather-icons"]'

# Security
MAX_FILE_SIZE_MB=100
MAX_REQUESTS_PER_MINUTE=10

# Logging
VERBOSE=false
