RETRIEVAL_ALPHA=0.8
TIMEOUT=5000

# ========== LLM Configuration ==========
# Thinking Mode Support:
# - qwen3-vl-flash: ‚úÖ Supports thinking=true/false
# - qwen3-vl-plus: ‚úÖ Supports thinking=true/false
# - qwen3-vl-235b-a22b-instruct: ‚ùå Does NOT support thinking (parameter ignored)
# - qwen3-vl-235b-a22b-thinking: üîí ALWAYS uses thinking (cannot disable)

# Qwen
DEFAULT_API_KEY=
DEFAULT_MODEL=qwen3-vl-plus
DEFAULT_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# Doubao
DEFAULT_API_KEY=
DEFAULT_MODEL=doubao-seed-1-6-250615
DEFAULT_BASE_URL=https://ark.cn-beijing.volces.com/api/v3

DEFAULT_TEMPERATURE=0.5
DEFAULT_TOP_K=
DEFAULT_TOP_P=
DEFAULT_ENABLE_THINKING=true
DEFAULT_THINKING_BUDGET=500
DEFAULT_VL_HIGH_RESOLUTION=false
# Global default max tokens for all stages (overridable per-stage)
DEFAULT_MAX_TOKENS=

