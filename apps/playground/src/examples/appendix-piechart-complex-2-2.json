{
  "widget": {
    "backgroundColor": "#FFFFFF",
    "borderRadius": 20,
    "padding": 24,
    "aspectRatio": 1,
    "root": {
      "type": "container",
      "direction": "col",
      "gap": 24,
      "flex": 1,
      "props": { "style": { "border": "1px solid #E5E5E5" } },
      "children": [
        {
          "type": "container",
          "direction": "row",
          "alignMain": "between",
