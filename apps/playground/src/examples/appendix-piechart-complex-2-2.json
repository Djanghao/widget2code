              "children": [
                { "type": "leaf", "component": "Text", "props": { "fontSize": 12, "color": "#888" }, "content": "Essential" },
                { "type": "leaf", "component": "Text", "props": { "fontSize": 16, "fontWeight": 600, "color": "#000" }, "content": "65%" }
              ]
            },
            {
              "type": "container",
              "direction": "col",
              "gap": 4,
              "alignCross": "end",
              "children": [
                { "type": "leaf", "component": "Text", "props": { "fontSize": 12, "color": "#888" }, "content": "Discretionary" },
                { "type": "leaf", "component": "Text", "props": { "fontSize": 16, "fontWeight": 600, "color": "#000" }, "content": "35%" }
