          "flex": 1,
          "alignMain": "center",
          "alignCross": "center",
          "children": [
            {
              "type": "leaf",
              "component": "PieChart",
              "props": {
                "data": [65, 35],
                "colors": ["#000000", "#E5E5E5"],
