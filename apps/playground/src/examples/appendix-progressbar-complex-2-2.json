{
  "widget": {
    "backgroundColor": "#FFFFFF",
    "borderRadius": 20,
    "padding": 32,
    "aspectRatio": 1,
    "root": {
      "type": "container",
      "direction": "col",
      "gap": 40,
      "flex": 1,
      "alignMain": "center",
      "children": [
        {
          "type": "container",
          "direction": "col",
          "gap": 16,
          "children": [
            { "type": "leaf", "component": "Text", "props": { "fontSize": 48, "fontWeight": 800, "color": "#000000", "lineHeight": 1 }, "content": "75%" },
            { "type": "leaf", "component": "Text", "props": { "fontSize": 16, "fontWeight": 600, "color": "#000000" }, "content": "Project Completion" }
          ]
        },
        {
          "type": "leaf",
          "component": "ProgressBar",
          "props": {
            "value": 75,
            "color": "#000000",
            "backgroundColor": "#F0F0F0",
            "height": 24,
            "style": { "borderRadius": 0 }
          }
        },
        {
          "type": "container",
          "direction": "row",
          "gap": 32,
          "children": [
            {
              "type": "container",
              "direction": "col",
              "gap": 4,
              "children": [
                { "type": "leaf", "component": "Text", "props": { "fontSize": 12, "color": "#888888" }, "content": "Tasks" },
                { "type": "leaf", "component": "Text", "props": { "fontSize": 16, "fontWeight": 600, "color": "#000000" }, "content": "12/16" }
              ]
            },
            {
              "type": "container",
              "direction": "col",
              "gap": 4,
              "children": [
                { "type": "leaf", "component": "Text", "props": { "fontSize": 12, "color": "#888888" }, "content": "Days Left" },
                { "type": "leaf", "component": "Text", "props": { "fontSize": 16, "fontWeight": 600, "color": "#000000" }, "content": "4" }
              ]
            }
          ]
        }
      ]
    }
  }
}
